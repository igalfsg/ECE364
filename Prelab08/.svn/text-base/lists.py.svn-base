import listmod


fvals = input('Enter the fist list of numbers: ')
svals = input('Enter the second list of numbers: ')
flist = fvals.split()
for i in range(len(flist)):
    flist[i] = int(flist[i])
slist = svals.split()
for i in range(len(slist)):
    slist[i] = int(slist[i])
print("First list: " ,flist)
print("Second list: " ,flist)


median, lis = listmod.find_median(slist, flist)


print ("Merged list: ", lis)
print ("Median: ", lis[median])