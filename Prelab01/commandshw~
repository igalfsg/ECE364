#! /bin/bash
#$Author$
#$Date$
#$HeadURL$
#$Revision$


if [[ -e $1 ]]
then
echo "found"
else
echo "shit"
fi

if [[ -r $1 && -w $1 ]]
then
echo "r and  w"
else
echo "shit"
fi

#executable
if [[ -x $1 ]]
then
echo "r and  w"
else
echo "shit"
fi
#directory (file stuff in lecture 1 page 27)
if [[ -d $1 ]]
then
echo "r and  w"
else
echo "shit"
fi

#padd a pritn statement
printf "%50s\n" string
